<!DOCTYPE html>
<html>
  <head>
    <title>Seasons</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <% flash.each do |key, value| %>
      <%= flash_msgs(key, value) %>
    <% end %>
    <%= yield %>
    <footer>
      <%= render 'shared/footer' %>
    </footer>  
  </body>
</html>
